    {n:"SHEIN è‘¡è„ç‰™", u:"https://pt.shein.com", t:["æ¬§æ´²","è‘¡è¯­"], c:"shein"},
    {n:"SHEIN ç‘å…¸", u:"https://se.shein.com", t:["åŒ—æ¬§"], c:"shein"},
    {n:"SHEIN æ³¢å…°", u:"https://pl.shein.com", t:["ä¸œæ¬§"], c:"shein"},
